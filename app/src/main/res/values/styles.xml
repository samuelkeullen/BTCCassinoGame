<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="DuplicateDefinition">

    <style name="AppTheme" parent="android:Theme.NoTitleBar" />

    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">#757575</item>
    </style>

    <style name="button_bar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:splitMotionEvents">false</item>
        <item name="android:padding">3dp</item>
        <item name="android:baselineAligned">false</item>
    </style>

    <style name="button">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_marginStart">3dp</item>
        <item name="android:layout_marginEnd">3dp</item>
        <item name="android:textColor">@color/button_text_color_active</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="button_bj" parent="button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/button_dark</item>
    </style>

    <style name="button_small" parent="button">
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="payout_text">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:paddingTop">0px</item>
        <item name="android:paddingBottom">0px</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:layout_marginTop">0px</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:lineSpacingMultiplier">1</item>
        <item name="android:lineSpacingExtra">0dp</item>
        <item name="android:layout_marginBottom">0px</item>
        <item name="android:background">@color/vp_payout_bg</item>
        <item name="android:textColor">@color/vp_payout_text</item>
    </style>

    <style name="d_input_row">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">120</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="d_input_label_container">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">130dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="d_input_label">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="d_input_label_units">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue_green</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="d_input_value">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:text">1</item>
        <item name="android:inputType">number</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:textColor">@color/blue_green</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="d_input_seekbar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:progressDrawable">@drawable/progressbar</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:progress">20</item>
        <item name="android:max">1000</item>
    </style>

    <style name="d_result_box">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">monospace</item>
        <item name="android:padding">7dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:background">@drawable/result_box</item>
    </style>

    <style name="d_lucky_number_digit">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">0dp</item>
        <item name="android:typeface">monospace</item>
    </style>

    <style name="d_error_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ff0000</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/error_box</item>
    </style>

    <style name="d_roll_button_label">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center_horizontal|top</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="d_roll_button_goal">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>